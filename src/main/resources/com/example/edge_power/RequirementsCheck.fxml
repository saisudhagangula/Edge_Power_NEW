<VBox prefHeight="409.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.edge_power.RequirementsCheckController">
    <children>
        <Label fx:id="statusLabel" prefHeight="18.0" prefWidth="479.0" text="Checking requirements..." />
        <Button fx:id="retryButton" onAction="#handleRetryButton" text="Retry" visible="false" />
    </children>
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
</VBox>
